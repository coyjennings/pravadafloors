        background-repeat: no-repeat;
        background-position: center right 5%;
    }

    #footer-moodboard input[type="submit"]:hover {
        background-color: #000;
        background-image: url("https://cdn.shopify.com/s/files/1/0422/9274/0261/files/moodboard-no-border-white.png?v=1597878997");
    }

    .close-container {
        position: absolute;
        right: 40px;
        top: -44px;
        background-color: #fff;
        font-size: 26px;
        transform: rotate(90deg);
        padding: 8px 16px 8px;
        border-radius: 25px;
        box-shadow: -1px 0px 0px rgba(0, 0, 0, 0.40);
        cursor: pointer;
    }

    .f-up-mb .close-container {
        transform: rotate(-90deg);
        text-align: center;
        padding: 6px 14px 10px;
        box-shadow: 1px 0px 1px rgba(0, 0, 0, 0.40);
    }

    @media screen and (max-width: 767px) {
        #footer-moodboard-form {
            max-width: 198px;
            margin: 0 auto;
        }
