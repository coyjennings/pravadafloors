<script>
    filterSelectionAll()

    function filterSelectionAll() {
        var x, i;
        x = document.getElementsByClassName("type-product");
        for (i = 0; i < x.length; i++) {
            w3AddClass(x[i], "filtered_all");
        }
    }

    function filterSelectionTypeEnginnered() {
        var x, z, i;
        x = document.getElementsByClassName("type-product");
        z = document.getElementById("wpf_wpf_5e4e36f34b91a_116");
        for (i = 0; i < x.length; i++) {
            w3RemoveClass(x[i], "filtered_all");
            if (x[i].className.indexOf("type_engineered") > -1) {
                if (z.checked == true) {
                    w3AddClass(x[i], "filtered_type_engineered");
                } else {
                    w3RemoveClass(x[i], "filtered_type_engineered");
                }
            }
        }
    }

    function filterSelectionTypeWaterproofSPC() {
        var x, z, i;
        x = document.getElementsByClassName("type-product");
        z = document.getElementById("wpf_wpf_5e4e36f34b91a_161");
        for (i = 0; i < x.length; i++) {
            w3RemoveClass(x[i], "filtered_all");
            if (x[i].className.indexOf("type_waterproof-spc") > -1) {
                if (z.checked == true) {
                    w3AddClass(x[i], "filtered_type_waterproof-spc");
                } else {
                    w3RemoveClass(x[i], "filtered_type_waterproof-spc");
